{
    "name": "simple_webrtc_nodejs_client",
    "version": "0.1.0",
    "description": "WebRTC client for Node JS using node-webrtc",
    "main": "server.js",
    "repository": "git@github.com:aljanabim/simple_webrtc_nodejs_client.git",
    "author": "Mustafa Al-Janabi",
    "license": "MIT",
    "scripts": {
        "start": "env-cmd -f ./config/prod.env node server.js",
        "dev": "env-cmd -f ./config/dev.env nodemon server.js",
        "chat": "env-cmd -f ./config/dev.env node examples/chat/server.js",
        "ros": "env-cmd -f ./config/dev.env node examples/ros/server.js"
    },
    "dependencies": {
        "inquirer": "^8.1.0",
        "roslib": "^1.1.0",
        "nodemon": "^2.0.4",
        "socket.io-client": "^4.0.1",
        "wrtc": "^0.4.7",
        "yargs": "^17.0.1"
    },
    "devDependencies": {
        "env-cmd": "^10.1.0"
    },
    "keywords": [
        "client",
        "webrtc",
        "peer-to-peer",
        "signaling",
        "socket.io",
        "websocket"
    ],
    "engines": {
        "node": "14.x"
    }
}
